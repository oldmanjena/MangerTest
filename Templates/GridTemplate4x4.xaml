<UserControl x:Class="MangerTest.Templates.GridTemplate4x4"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MangerTest.Templates"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="120"/>
        </Grid.ColumnDefinitions>

        <ContentPresenter Grid.Row="0" Grid.Column="0" Content="{Binding Cell00}"/>
        <ContentPresenter Grid.Row="0" Grid.Column="1" Content="{Binding Cell01}"/>
        <ContentPresenter Grid.Row="0" Grid.Column="2" Content="{Binding Cell02}"/>
        <ContentPresenter Grid.Row="0" Grid.Column="3" Content="{Binding Cell03}"/>

        <ContentPresenter Grid.Row="1" Grid.Column="0" Content="{Binding Cell10}"/>
        <ContentPresenter Grid.Row="1" Grid.Column="1" Content="{Binding Cell11}"/>
        <ContentPresenter Grid.Row="1" Grid.Column="2" Content="{Binding Cell12}"/>
        <ContentPresenter Grid.Row="1" Grid.Column="3" Content="{Binding Cell13}"/>

        <ContentPresenter Grid.Row="2" Grid.Column="0" Content="{Binding Cell20}"/>
        <ContentPresenter Grid.Row="2" Grid.Column="1" Content="{Binding Cell21}"/>
        <ContentPresenter Grid.Row="2" Grid.Column="2" Content="{Binding Cell22}"/>
        <ContentPresenter Grid.Row="2" Grid.Column="3" Content="{Binding Cell23}"/>

        <ContentPresenter Grid.Row="3" Grid.Column="0" Content="{Binding Cell30}"/>
        <ContentPresenter Grid.Row="3" Grid.Column="1" Content="{Binding Cell31}"/>
        <ContentPresenter Grid.Row="3" Grid.Column="2" Content="{Binding Cell32}"/>
        <ContentPresenter Grid.Row="3" Grid.Column="3" Content="{Binding Cell33}"/>

    </Grid>
</UserControl>
